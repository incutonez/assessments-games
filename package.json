{
  "name": "@incutonez/assessments-games",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "npx eslint --fix",
    "preview": "vite preview",
    "explode": "npm cache clean --force && npx rimraf package-lock.json **/node_modules --glob && npm i",
    "prepare": "husky",
    "update:deps": "node ./updateDependencies.js",
    "update:versions": "node ./updateVersions.js"
  },
  "dependencies": {
    "@material-symbols/svg-400": "^0.31.9",
    "@tanstack/react-router": "^1.121.41",
    "classnames": "^2.5.1",
    "react": "^19.1.0",
    "react-aria-components": "^1.10.1",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "latest",
    "@incutonez/eslint-plugin": "^2.1.4",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^7.1.0",
    "@semantic-release/git": "^10.0.1",
    "@stylistic/eslint-plugin-ts": "^4.4.1",
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/router-plugin": "^1.121.41",
    "@types/node": "^24.0.4",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "@vitejs/plugin-react": "^4.6.0",
    "conventional-changelog-conventionalcommits": "^9.0.0",
    "eslint": "^9.29.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "semantic-release": "^24.2.5",
    "tailwindcss": "latest",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.0",
    "vite": "^7.0.0",
    "vite-plugin-svgr": "^4.3.0"
  },
  "lint-staged": {
    "src/**/*.{tsx,ts}": "npm run lint"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "conventionalcommits"
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "conventionalcommits"
        }
      ],
      "@semantic-release/changelog",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  }
}
